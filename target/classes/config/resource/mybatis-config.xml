<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <!-- changes from the defaults -->
        <setting nickName="lazyLoadingEnabled" value="false"/>
    </settings>
    <plugins>
        <plugin interceptor="com.tuniu.nfbird.core.db.mybatis.plugins.PaginationPlugin">
            <property nickName="dialect" value="mysql"/>
        </plugin>
    </plugins>

</configuration>
